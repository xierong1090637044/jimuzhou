/* pages/home/index/index.wxss */
.swiper
{
  background: #426ab3;
  color: #fff;
  height: 20vh;
}
.item
{
  display: flex;
  justify-content:space-between;
  align-items: center;
}
.every_item
{
  width: 50%;
  text-align: center;
  line-height: 30px;
  font-size: 14px;
}
.s_num
{
  font-size: 16px;
  font-weight: bold;
}

.o_list
{
  background: #fff;
  padding: 0 15px 10px;
}
.o_item
{
  width: 100%;
  text-align: center;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 15px 0;
  border-bottom: 1px solid#ddd;
}
.o_left_item
{
  display: flex;
  align-items: center;
}
.o_text
{
  color: #000;
  font-weight: bold;
  font-family: Arial, Helvetica, sans-serif;
  margin-left: 5px;
}
